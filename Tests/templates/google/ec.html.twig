{% spaceless %}
  {{ cjs_remove_block('google_analytics', 'page_view') }}
  {{ cjs_add_block('google_analytics', 'ec/init') }}
  {% set action = cjs_model('google_analytics', 'ec_action', ['click', 'no link']) %}
  {{ cjs_add_block('google_analytics', 'ec/set_action', null, false, {action: action}) }}
  {% set impression = cjs_model('google_analytics', 'ec_impression', ['SKU1', 'A Book', "Books"]) %}
  {{ cjs_add_block('google_analytics', 'ec/add_impression', null, false, {impression: impression}) }}
  {% set product = cjs_model('google_analytics', 'ec_product', ['SKU1', 'A Book']) %}
  {{ cjs_add_block('google_analytics', 'ec/add_product', null, false, {product: product}) }}
  {% set promo = cjs_model('google_analytics', 'ec_promo', ['PROMO1', 'The Best Promotion Ever']) %}
  {{ cjs_add_block('google_analytics', 'ec/add_promo', null, false, {promo: promo}) }}
  {{ cjs_add_block('google_analytics', 'page_view') }}
  {{ cjs_js('google_analytics') }}
{% endspaceless %}
